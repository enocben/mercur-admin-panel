{
  "name": "admin-panel",
  "version": "1.5.1",
  "scripts": {
    "generate:static": "node ./scripts/generate-currencies.js && prettier --write ./src/lib/currencies.ts",
    "dev": "vite",
    "build": "tsup && node ./scripts/generate-types.js",
    "build:preview": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "i18n:validate": "node ./scripts/i18n/validate-translation.js",
    "i18n:schema": "node ./scripts/i18n/generate-schema.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "main": "dist/app.js",
  "module": "dist/app.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/app.mjs",
      "require": "./dist/app.js",
      "types": "./dist/index.d.ts"
    },
    "./css": {
      "import": "./dist/app.css",
      "require": "./dist/app.css"
    },
    "./root": "./",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medusajs/medusa",
    "directory": "packages/admin/dashboard"
  },
  "files": [
    "package.json",
    "src",
    "dist"
  ],
